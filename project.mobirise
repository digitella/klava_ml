        {
          "alias": "amp-blocks/components",
          "_isSecondary": true,
          "typeCompatible": "isAMP",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              "& > .mbr-overlay": {
                "background-color": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".title-wrap, .title": {
              "width": "100%"
            },
            "*": {
              "word-break": "break-word"
            },
            ".items-list-wrap .list-item": {
              "& when (@strippedBg) and not (@transparentBg)": {
                "&:nth-child(odd)": {
                  "background-color": "@strippedBgColor"
                }
              },
              "& when (@showIcons)": {
                "display": "flex",
                "align-items": "center"
              }
            },
            "& when (@showIcons)": {
              ".iconfont-wrapper": {
                "margin-right": "1rem",
                "width": "~\"@{iconsSize}rem\"",
                "height": "~\"@{iconsSize}rem\""
              },
              ".amp-iconfont": {
                "color": "@primaryColor",
                "font-size": "~\"@{iconsSize}rem\"",
                "width": "~\"@{iconsSize}rem\"",
                "vertical-align": "middle"
              }
            },
            "& when not (@transparentBg)": {
              "& when not (@checkDifferentBg)": {
                ".card-inner, .card-head, .card-price-wrap, .list-item": {
                  "background-color": "@cardBg"
                }
              },
              "& when (@checkDifferentBg)": {
                ".card1": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card1Bg"
                  }
                },
                ".card2 when (@cardsAmount > 1)": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card2Bg"
                  }
                },
                ".card3 when (@cardsAmount > 2)": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card3Bg"
                  }
                },
                ".card4 when (@cardsAmount > 3)": {
                  ".card-inner, .card-head, .card-price-wrap, .list-item": {
                    "background-color": "@card4Bg"
                  }
                }
              }
            },
            "& when (@transparentBg)": {
              ".card-inner, .card-price-wrap, .list-item": {
                "background": "transparent"
              },
              ".card-inner": {
                "position": "relative",
                ".mbr-overlay": {
                  "opacity": "@bgOpacity",
                  "background-color": "@cardBg"
                },
                "div:not(.mbr-overlay)": {
                  "z-index": "1"
                }
              }
            },
            "& when (@cardBorder)": {
              ".card-price-wrap": {
                "border-top": "1px solid @colorBorder",
                "border-bottom": "1px solid @colorBorder"
              }
            },
            ".card-price-wrap": {
              "align-items": "baseline"
            },
            ".card-price sup, .card-price span, .price-caption": {
              "color": "@primaryColor"
            },
            ".card-inner": {
              "&:after": {
                "content": "\"\"",
                "position": "absolute",
                "left": "0",
                "bottom": "0",
                "display": "block",
                "height": "2px",
                "width": "100%",
                "transform": "scale(0)",
                "transition": "transform 1s",
                "background-color": "@primaryColor"
              },
              "&:hover:after": {
                "transform": "scale(1)"
              }
            },
            ".price-value": {
              "font-size": "3rem"
            },
            ".items-list-wrap": {
              "padding": "1.56rem 0"
            },
            ".custom-card-btn": {
              "padding": "13px 30px"
            },
            ".card-head1": {
              "background-color": "@card1Color"
            },
            ".card-head2 when (@cardsAmount > 1)": {
              "background-color": "@card2Color"
            },
            ".card-head3 when (@cardsAmount > 2)": {
              "background-color": "@card3Color"
            },
            ".card-head4 when (@cardsAmount > 3)": {
              "background-color": "@card4Color"
            },
            ".card1 .card-title": {
              "color": "#ffffff"
            },
            ".card2 .card-title": {
              "color": "#f4f5f7"
            },
            ".card3 .card-title": {
              "color": "#f4f5f7"
            },
            ".card4 .card-title": {
              "color": "#f4f5f7"
            }
          },
