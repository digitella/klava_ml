                    "margin-right": "8px",
                    "width": "~\"@{iconsSize}rem\"",
                    "height": "~\"@{iconsSize}rem\"",
                    "&:before, &:after": {
                      "content": "\"\"",
                      "position": "absolute",
                      "background-color": "@primaryColor"
                    },
                    "&:before": {
